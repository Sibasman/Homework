import math
while True:
   try:
      homework = 5
      
      print(f"Доступно домашних заданий: {homework}")
      
      data = int(input("Введите задание: "))
   
      if data == 1:
         
         print("Это задание на Match-case")
         
         case_data_one = int(input(f"Введите первое значение: "))
         case_data_two = int(input(f"Введите второе значение: "))
         char = input(f"Введите арифметический оператор. ")
   
         match char:
            
            case '+':
                  print(case_data_one + case_data_two)
                  
            case '-':
               print(case_data_one - case_data_two)
               
            case '*':
               print(case_data_one * case_data_two)
               
            case '/':
             print(case_data_one / case_data_two)
             
            case '//':
             print(case_data_one // case_data_two)
             
            case '%':
               print(case_data_one % case_data_two)
               
            case '**':
               print(case_data_one ** case_data_two)
               
            case '<<':
               print(case_data_one << case_data_two)
               
            case '>>':
               print(case_data_one >> case_data_two)
               
            case '^':
               print(case_data_one ^ case_data_two)
               
            case '&':
               print(case_data_one & case_data_two)
               
            case '|':
               print(case_data_one | case_data_two)
               
            case _:
               print(f"Все говно, давай по новой")
               continue
      
      if data == 2:
         
         print(f"Это задание на Ternary if")
         
         print(f"Какой тип задания хотите проверить?")
         
         print(f"Число четное не четное или булевое значение. (0/1): ")
         
         task = int(input("Ответ: "))
         
         if task == 0:
         
            char = int(input(f"Введите число: "))
         
            if char % 2 == 0:
               print(f"Число четное, так как остаоток {char % 2}")
               
            else:
               print(f"Число не четное, так как остаток {char % 2}")
         elif task == 1:
            
            print(f"Вы любите печеньки?")
            
            char_try = int(input("Ответ да или нет (1/0):"))       
            
            1 == True
            0 == False
            
            if bool(char_try):
               print(f"Печеньки сила!")
               
            else:
               print(f"Ну ладно")
               
      if data == 3:
         
         print(f"Это задание типа Nested IF")
         
         print(f"Консультант: Добрый день, какой у вас тип приёма?")
         type_one = input("Введите тип приема (Экстренный/Плановый): ").lower
         
         if type_one == "Экстренный":
            print(f"Консультант: Вы идете в не очереди, держите печеньку)")
            
         if type_one == "Плановый":
            print(f"Консультант: Ожидайте")
            type_two = input("Есть ли запись (Да/Нет): ").lower
            
            if type_two == "Да":
               print(f"Консультант: когда вы прибыли?")
               type_tree = int(input("Время прибытия: "))
               
               if type_tree <= 12:
                  print(f"Консультант: Вам повезло, встаньте в очередь, вас позже примут.")
                  
               else:
                  print(f"Консультант: Сожалею, но приходите завтра, сегодня все уже забито.")
                  
            else:
               print(f"Консультант: Сожалею, но без записи не принимаем.")
               
         else:
            print(f"Ошибка.")

      if data == 4:
         
         print(f"Тут задачи типа For Cycle")
         
         task = int(input(f"Введите какая задача вам надо (0/4): "))
         
         if task == 0:
         
            for char in range(501):
               total_char = 0
               temp_char = char
               while temp_char > 0:
                  total_char += temp_char % 10
                  temp_char //= 10
               print(f"Получившиеся число: {char}, сумма цифр: {total_char}")
         
         if task == 1:
            
            for char in range(1001):
               even_sum = 0
               add_sum = 0
               
               #для упрощение пришлось число сделать временно строкой, что бы проще было вывести нужные значения
               for i, char_one in enumerate(str(char)):
                  char_one = int(char_one)
                  
                  if i % 2 == 0:
                     even_sum += char_one
                     
               else:
                  add_sum += char_one
                  
               if even_sum == add_sum:
                  print(f"Число: {char}, сумма чётных позиций: {even_sum}, нечётных: {add_sum}")
                  
         if task == 2:
            
            N = int(input("Введите число: "))
            for i in range(1, N + 1):
               if N % i == 0:
                  print(i)
                  
         if task == 3:
            
            N = int(input("Введите число: "))
            
            for i in range(11):
               print(f"{N} * {i} = {N * i}")

      else:
         print(f"Некоректный ввод данных.")
   except ValueError:
      print(f"Некоректный ввод данных.")
